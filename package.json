{
  "name": "@dastbal/nestjs-ai-agent",
  "version": "1.0.8",
  "description": "Autonomous AI Agent for NestJS - Principal Software Engineer Level with RAG and SQLite persistence",
  "author": "David Balladares",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent": "dist/bin/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "gen": "ts-node src/bin/cli.ts",
    "agent": "ts-node src/bin/cli.ts graph",
    "start:dev": "ts-node src/bin/cli.ts",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@langchain/core": "^1.1.18",
    "@langchain/google-vertexai": "^2.1.6",
    "@langchain/langgraph": "^1.0.7",
    "@langchain/langgraph-checkpoint-sqlite": "^1.0.0",
    "better-sqlite3": "^12.5.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.2",
    "deepagents": "^1.6.1",
    "dotenv": "^17.2.3",
    "langchain": "^1.2.16",
    "sqlite3": "^5.1.7",
    "ts-morph": "^27.0.2",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "devDependencies": {
    "@langchain/langgraph-cli": "^1.1.11",
    "@nestjs/common": "^10.4.22",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.30",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}